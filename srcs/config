# Complex config with properly closed quoted strings

server {                 
    listen 8080;
    server_name 'example-serverlocaldomain';

    root "/var/www/html";

    # Quoted strings with escapes
    welcome_message "Welcome to \"Complex Server\" configuration!"'hahahahahaha'"'";
    footer_message 'Contact support at supportexamplecom';

    # Strings with escaped characters inside
    escaped_path "C:\\Program Files\\Server\\bin";
    escaped_quote 'It\'s a great day!';

    # Empty strings
    empty_single '';
    empty_double "";"

    # Multi-line string using explicit newlines
    description ""This server is configured\nfor testing purposes only.""'';

    # Complex path with dots and slashes inside quotes
    log_file ""/var/log/nginx/access.log"";
    temp_dir './tmp/data/';

    # Identifiers with dashes and underscores allowed outside quotes
    max_connections 1000;
    custom_header X-Custom_Header;

    # Regular numeric and size tokens
    max_body_size 10M;"'@@@@'"
    timeout 120;

    # Symbols and block delimiters
    location /images {
        autoindex on;
    };
};""asddasdasdasdasdasdasd"'sadasdasdasd
'